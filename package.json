{
  "name": "tabnews-cli",
  "module": "index.ts",
  "type": "module",
  "version": "1.0.1",
  "description": "A simple news app on the terminal",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "scripts": {
    "dev": "bun run ./src/index.ts",
    "build": "npx tsup src --format esm --minify",
    "start": "node ./dist/index.js",
    "build:start": "npm run build && npm run start",
    "biome:check": "biome check ./src",
    "biome:check-apply": "biome check --apply ./src",
    "biome:check-apply-unsafe": "biome check --apply-unsafe ./src",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@types/bun": "latest",
    "tsup": "^8.0.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@types/inquirer": "^9.0.7",
    "commitizen": "^4.3.0",
    "husky": "^9.0.11",
    "inquirer": "^9.2.15",
    "terser": "^5.28.1"
  }
}